(defthm p2
  (implies (and (booleanp b0)
		(booleanp b1)
		(booleanp b2)
		(booleanp b3)

		(ste-thm *nand-delay*
			 `(and (and (is 0 ,b0)
				    (is 2 ,b2))
			       (next (next (is 3 nil))))
			 `(and (next (next (is 2 ,(or (not b0) (not b2)))))
			       (next (next (is 3 nil))))
			 )
		(ste-thm *nand-delay*
			 `(and (and (is 1 ,b1)
				    (next (is 0 ,b3)))
			       (next (next (is 3 nil))))
			 `(and (next (next (is 1 ,(not (and b1 b3)))))
			       (next (next (is 3 nil))))
			 )
