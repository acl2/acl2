			    (c `(and (and (next (next (is 2 ,(or (not b0) (not b2)))))
					  (next (next (is 3 nil))))
				     (and (next (next (is 1 ,(not (and b1 b3)))))
					  (next (next (is 3 nil))))
				     ))
			    (a1 `(and (and (and (is 0 ,b0)
						(is 1 ,b1))
					   (and (is 2 ,b2)
						(next (is 0 ,b3))))
				      (next (next (is 3 nil)))))
			    (c1 `(and (and (next (next (is 1 ,(not (and b1 b3)))))
					   (next (next (is 2 ,(or (not b0) (not b2))))))
				      (next (next (is 3 nil))))))
		 ))))


(defthm nand-delay-2cycle
