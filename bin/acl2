#!/bin/sh

{ pushd $(dirname "$0") ; invoking_dir="`pwd`" ; popd ; } > /dev/null
my_acl2=$(dirname "$invoking_dir")/saved_acl2
if [ -x "$my_acl2" ] ; then \
    "$my_acl2" "$@" ;\
elif [ -e "$my_acl2" ] ; then \
    echo "Error: File \"$my_acl2\" is not an executable file." ;\
else \
    echo "Error: File \"$my_acl2\" does not exist." ;\
fi
